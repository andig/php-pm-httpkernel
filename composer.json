{
    "name": "php-pm/httpkernel-adapter",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php-pm/php-pm": "dev-react-0.8",
        "symfony/http-foundation": "^2.6|^3.0",
        "symfony/http-kernel": "^2.6|^3.0",
        "ringcentral/psr7": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "PHPPM\\": ""
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/andig/php-pm"
        }
    ],
    "config": {
       "platform": {
           "php": "5.6"
       }
    }
}
